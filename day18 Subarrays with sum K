 int countSubarrays(vector<int> &arr, int k) {
        // code here
        int sum=0,cnt=0;
        unordered_map<int,int> mpp;
        mpp[0]=1;
        for(int i=0;i<arr.size();i++){
            sum+=arr[i];
            cnt+=mpp[sum-k];
            mpp[sum]++;
        }
        return cnt;
    }
